{
    "name": "cutcha-backend",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "description": "This is the cutcha backend",
    "main": "dist/server.js",
    "scripts": {
        "start": "tsc && node -r dotenv/config dist/server.js",
        "start:prod": "node dist/server.js",
        "build": "tsc",
        "watch:only": "concurrently \"tsc -w\" \"nodemon -r dotenv/config dist/server.js\"",
        "watch:both": "concurrently \"cd ../frontend && npm start\" \"tsc -w\" \"nodemon -r dotenv/config dist/server.js\""
    },
    "keywords": [],
    "author": "HeleleF",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.21.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-rate-limit": "^5.2.6",
        "express-validator": "^6.10.0",
        "helmet": "^4.4.1",
        "mongoose": "^5.12.1"
    },
    "devDependencies": {
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.10",
        "@types/errorhandler": "^1.5.0",
        "@types/express": "^4.17.11",
        "@types/express-rate-limit": "^5.1.1",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "concurrently": "^6.0.0",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "eslint": "^7.22.0",
        "nodemon": "^2.0.7",
        "typescript": "^4.2.3"
    }
}
